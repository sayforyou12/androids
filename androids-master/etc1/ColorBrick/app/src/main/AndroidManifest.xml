<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.android.colorbrick">
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CBStartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".CBMainActivity">
        </activity>
        <activity android:name=".CBClearActivity">
        </activity>
        <activity android:name=".CBOverActivity">
        </activity>
        <activity android:name="intromain.IntroActivity">
        </activity>
    </application>

</manifest>